{
  "name": "VueHelperInterfaces",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "serve": "vue-cli-service serve",
    "build": "npm run build:lib",
    "lint": "vue-cli-service lint",
    "build:default": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib src/index.js",
    "test:unit": "vue-cli-service test:unit -w",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-types": "^1.3.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-notes": "^3.4.10",
    "@storybook/vue": "^3.4.10",
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-e2e-nightwatch": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-standard": "^3.0.0-rc.10",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "^6.26.3",
    "chai": "^4.1.2",
    "eslint-plugin-vue-types": "^0.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "storybook-addon-vue-info": "^0.6.0",
    "vue-storybook": "^0.4.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:vue/base",
      "@vue/standard",
      "plugin:vue-types/strongly-recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist"
  ],
  "module": "dist/VueHelperInterfaces.umd.js",
  "main": "dist/VueHelperInterfaces.common.js",
  "unpkg": "dist/VueHelperInterfaces.umd.js"
}
